{
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "es2020",
    "target": "es2020",
    "jsx": "preserve",
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "baseUrl": "./www/",
    "moduleResolution": "node",
    "paths": {
        //"build/*":["static/build/*"],
        "*": ["static/build/_snowpack/pkg/*", "static/build/*", "../node_modules/*", "node_modules/*","*"]
    },
  },
}
